{
  "name": "hashup",
  "type": "module",
  "version": "0.4.5",
  "packageManager": "pnpm@8.15.7",
  "description": "CLI to hash build asset files",
  "author": {
    "name": "Johann Schopplich",
    "email": "hello@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/hashup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/hashup.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/hashup/issues"
  },
  "keywords": [
    "kirby",
    "hash",
    "assets",
    "bundle"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "hashup": "./bin/hashup.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "picocolors": "^1.0.0",
    "tiny-glob": "^0.2.9",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  }
}
