{
  "name": "hashup",
  "version": "0.4.2",
  "packageManager": "pnpm@7.18.2",
  "description": "CLI to hash build asset files",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/hashup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/hashup.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/hashup/issues"
  },
  "keywords": [
    "kirby",
    "hash",
    "assets",
    "bundle"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "hashup": "./bin/hashup.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/node": "^18.11.16",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "consola": "^2.15.3",
    "eslint": "^8.30.0",
    "picocolors": "^1.0.0",
    "tiny-glob": "^0.2.9",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2"
  }
}
